  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/jest": "^29.5.11",
    "@types/mocha": "^10.0.6",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.9.1",
    "@types/sinon": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "chai": "^4.3.10",
    "eslint": "^8.53.0",
